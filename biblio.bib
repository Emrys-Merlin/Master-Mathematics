@article{Bader2006,
abstract = {We propose a new approach to superrigidity phenomena and implement it for lattice representations and measurable cocycles with homeomorphisms of the circle as the target group. We are motivated by Ghys' theorem stating that any representation {\$}\backslashro:\backslashGamma\backslashto\backslashHomeo{\_}+(S{\^{}}1){\$} of an irreducible lattice {\$}\backslashGamma{\$} in a semi-simple real Lie group {\$}G{\$} of higher rank, either has a finite orbit or, up to a semi-conjugacy, extends to {\$}G{\$} which acts through an epimorphism {\$}G\backslashto\backslashPSL{\_}{\{}2{\}}(\backslashRR){\$}. Our approach, based on the study of abstract boundary theory and, specifically, on the notion of a generalized Weyl group, allows: (A) to prove a similar superrigidity result for irreducible lattices in products {\$}G=G{\_}{\{}1{\}}\backslashtimes... G{\_}{\{}n{\}}{\$} of {\$}n\backslashge 2{\$} general locally compact groups, (B) to give a new (shorter) proof of Ghys' theorem, (C) to establish a commensurator superrigidity for general locally compact groups, (D) to prove first superrigidity theorems for {\$}\backslashtilde{\{}A{\}}{\_}{\{}2{\}}{\$} groups. This approach generalizes to the setting of measurable circle bundles; in this context we prove cocycle versions of (A), (B) and (D). This is the first part of a broader project of studying superrigidity via generalized Weyl groups.},
archivePrefix = {arXiv},
arxivId = {math/0605276},
author = {Bader, Uri and Furman, Alex and Shaker, Ali},
eprint = {0605276},
file = {:home/tim/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bader, Furman, Shaker - 2006 - Superrigidity, Weyl groups, and actions on the Circle.pdf:pdf},
mendeley-groups = {Master},
month = {may},
primaryClass = {math},
title = {{Superrigidity, Weyl groups, and actions on the Circle}},
%url = {http://arxiv.org/abs/math/0605276},
year = {2006}
}

